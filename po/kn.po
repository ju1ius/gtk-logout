msgid "Cancel"
msgstr "ರದ್ದು ಮಾಡು(C)"

msgid "Quit"
msgstr "ಬಿಟ್ಟು ಬಿಡು(Q)"

msgid "Hibernate"
msgstr "ಹೈಬರ್ನೇಟ್"

msgid "Lock Screen"
msgstr "ತೆರೆಯನ್ನು ಲಾಕ್ ಮಾಡು"

msgid "Log out"
msgstr "ಹೊರಗೆ ನಡೆ"

msgid "Restart"
msgstr "ಪುನಃ ಆರಂಭಿಸು"

msgid "Shut Down"
msgstr "ಮುಚ್ಚು"

msgid "Suspend"
msgstr "ತಾತ್ಕಾಲಿಕ ಸ್ಥಗಿತ"

msgid "Switch User"
msgstr "ಬಳಕೆದಾರನನ್ನು ಬದಲಿಸು"

